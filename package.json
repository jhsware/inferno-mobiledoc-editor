{
  "name": "inferno-mobiledoc-editor",
  "version": "0.6.0",
  "description": "A Mobiledoc editor for Inferno.js apps",
  "main": "dist/main.js",
  "scripts": {
    "start": "parcel ./demo/index.html",
    "build-demo": "rm -rf ./demo/dist && cross-env NODE_ENV=development node_modules/.bin/webpack --config ./demo/webpack.config.js",
    "test": "echo 'todo'",
    "build": "npm run build:clean && npm run build:es && npm run build:dist && npm run build:dist:prod",
    "build:clean": "rm -rf dist/ && rm -rf lib/",
    "build:es": "cross-env NODE_ENV=development babel src --out-dir lib",
    "build:dist": "cross-env NODE_ENV=development rollup -c",
    "build:dist:prod": "cross-env NODE_ENV=production rollup -c",
    "lint": "eslint src",
    "prepare": "npm run build",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "babel-loader": "^7.1.5",
    "classnames": "^2.2.6",
    "inferno": "^5.4.2",
    "inferno-create-element": "^5.4.2",
    "mobiledoc-kit": "^0.11.1"
  },
  "peerDependencies": {},
  "devDependencies": {
    "autoprefixer": "^8.6.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^20.0.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-inferno": "^5.0.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-inferno-app": "^7.1.0",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.4.0",
    "eslint-config-eslint": "^5.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-inferno": "^7.10.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "husky": "^0.14.3",
    "jest": "^23.4.0",
    "lint-staged": "^7.2.0",
    "mobiledoc-kit": "^0.11.1",
    "node-sass": "^4.9.2",
    "parcel-bundler": "^1.9.6",
    "prettier": "^1.13.7",
    "prettier-eslint": "^8.8.2",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-babel-minify": "^5.0.0",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0"
  },
  "repository": "jhsware/inferno-mobiledoc-editor",
  "homepage": "https://github.com/jhsware/inferno-mobiledoc-editor",
  "bugs": "https://github.com/jhsware/inferno-mobiledoc-editor/issues",
